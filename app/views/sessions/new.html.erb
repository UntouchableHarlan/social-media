<div class="container">
  <%= form_for(:session, url: login_path) do |f| %>
    <%= f.label :email %>
    <%= f.email_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %>

    <%= f.submit 'Login' %>
  <% end %>
</div>
<div class="container">
  <p>
    Not signed up, <%= link_to 'Sign Up', signup_path %>
  </p>
</div>
